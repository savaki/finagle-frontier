{
  "decorators": {
    "default": {
      "content_type": "text/html",
      "uri": "/sample/default.html",
      "context": [
        {
          "name": "session",
          "uri": "/session/header",
          "timeout": 50
        },
        {
          "name": "title",
          "header": "X-Title"
        }
      ]
    }
  },
  "territories": [
    {
      "port": 9080,
      "trail": [
        {
          "locations": ["/signup/*"],
          "hosts": ["csop.loyal3.com:443"],
          "enable_tls": true,
          "tags": ["r192"]
        },
        {
          "hosts": ["www.loyal3.com:80"],
          "tags": ["r194"]
        }
      ]
    }
  ]
}
